; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BoardGames"
run/main_scene="res://scene/page/authentication/authentication.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
config/icon="res://icon.svg"

[autoload]

FireBaseConf="*res://script/singleton/FireBaseConf.cs"
Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"
BggService="*res://scene/singleton/bgg-service/bgg_service.tscn"
RealTimeUserService="*res://script/singleton/RealTimeUserService.cs"
UserService="*res://script/singleton/user_service.gd"
AppData="*res://script/singleton/app_data.gd"
Signals="*res://script/singleton/signals.gd"
Notifications="*res://scene/singleton/notifications/notifications.tscn"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=540
window/size/window_height_override=860
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[dotnet]

project/assembly_name="BoardGames"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-firebase/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "orange",
"res://asset/": "blue",
"res://resource/": "yellow",
"res://scene/": "purple",
"res://script/": "teal"
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
